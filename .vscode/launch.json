    "version": "0.2.0",
    "configurations": [
        {
            "name": "repo_search",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=FLAVOR=dev"
            ],
        },
        {
            "name": "repo_search (with device_preview)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=FLAVOR=dev",
                "--dart-define=enable_device_preview=true",
            ],
        },
        {
            "name": "repo_search (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define=FLAVOR=stg"
            ],
        },
        {
            "name": "repo_search (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define=FLAVOR=prod"
            ],
        }
    ]
}